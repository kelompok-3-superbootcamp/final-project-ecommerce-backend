                "tags": [
                    "auth"
                ],
                "summary": "Registering a user.",
                "operationId": "register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Adi Cahya S"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "adics@gmail.com"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "example": "62812345678910"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "pw"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "401": {
                        "description": "Invalid input",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "201": {
                        "description": "Register account Successfully",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
